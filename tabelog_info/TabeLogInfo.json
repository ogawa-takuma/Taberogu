<script id="js-tmpl-rvwr-item-for-encourage-follow" type="text/x-handlebars-template">
<div class="p-select-follow__rvwr-item">
  {{#if recommend}}
    {{#if same_bookmark_place}}
      <p class="p-select-follow__rvwr-keywords">
        <strong class="p-select-follow__rvwr-keywords-hit">
          {{same_bookmark_place}}
        </strong>
        に｢{{#if is_visited}}行った{{else}}行きたい{{/if}}｣しています。
      </p>
    {{/if}}
  {{/if}}

  {{#if famous_user}}
    <p class="p-select-follow__rvwr-keywords">
      <strong>
        {{other_name}}
      </strong>
    </p>
  {{/if}}

  <div class="p-related-rvwr-item">
    <p class="p-related-rvwr-item__rvwr-image">
      <a href="{{rvwr_url}}" target="_blank" class="rvwr-image rvwr-image--s">
        <img src="{{user_image_path}}" width="40" height="40" alt="{{nickname}}">
      </a>
    </p>
    <div class="p-related-rvwr-item__header">
      <div class="p-related-rvwr-item__info">
        <p class="p-related-rvwr-item__names">
          <a href="{{rvwr_url}}" target="_blank" class="p-related-rvwr-item__nickname">
            {{nickname}}
          </a>
          {{#if myfriend}}
            <span class="p-related-rvwr-item__fbname">
              <span class="c-rvwr-facebook-name">{{facebook_name}}</span>
            </span>
          {{/if}}
          {{#if celebrity}}
            <span class="p-related-rvwr-item__badge">
              <a href="https://tabelog.com/celebrity/" target="_blank">
                <span class="c-badge-celebrity">
                  <i>
                    グルメ著名人
                  </i>
                </span>
              </a>
            </span>
            <span class="p-related-rvwr-item__othername">
